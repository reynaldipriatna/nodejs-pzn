{
  "name": "belajar-nodejs-express",
  "version": "1.0.0",
  "description": "Belajar Node JS Express",
  "main": "./src/index.js",
  "type": "module",
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "verbose": true,
    "maxConcurrency": 5,
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "collectCoverage": false,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!vendor/**/*.{js,jsx}"
    ]
  },
  "keywords": [
    "nodejs",
    "unit-test",
    "test"
  ],
  "author": "Reynaldi Priatna",
  "license": "ISC",
  "dependencies": {
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "express": "^4.18.2",
    "cookie-parser": "^1.4.6",
    "mustache-express": "^1.3.2",
    "express-fileupload": "^1.4.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.23.4",
    "@babel/preset-env": "^7.23.3",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "supertest": "^6.2.4"
  }
}
